{{#each this}}
  <li>
    <img src='{{flags.svg}}' alt='{{name.official}}' />
    <h2>{{name.official}}</h2>
    <p>Capital:{{capital}}</p>
    <p>Population:{{population}}</p>
    <p>Languages:{{#each}}
        <li>{{languages}}</li>
      {{/each}}
    </p>
  </li>
{{/each}}

